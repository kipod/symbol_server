    "version": "0.2.0",
    "configurations": [
        {
            "name": "fix_ss",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "pythonPath": "${config:python.pythonPath}",
            "program": "${workspaceRoot}/fix_ss.py",
            "cwd": "${workspaceFolder}",
            "env": {},
            "envFile": "${workspaceFolder}/.env",
            "debugOptions": [
                "RedirectOutput"
            ],
            "args": [
                "--show-only",
                "--fix-server",
                "D:\\server.txt",
//                "--delete-defect-transactions"
                "--path-to-ss",
                "D:\\myserversymbols"
            ]
        }
    ]
}